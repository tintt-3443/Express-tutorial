{
  "name": "express-locallibrary",
  "version": "0.0.0",
  "scripts": {
    "start": "nodemon ts-node src/index.ts",
    "dev:start": "nodemon dist/index.js",
    "server:start": "DEBUG=express-locallibrary:* npm run dev:start",
    "lint": "eslint . --ext .ts",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d ./src/config/ormconfig.ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d src/config/ormconfig.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/config/ormconfig.ts"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "@types/connect-flash": "^0.0.40",
    "@types/express-session": "^1.18.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "i18next": "^23.10.1",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-middleware": "^3.5.0",
    "inserturlparams": "^1.0.1",
    "jet-logger": "^1.3.1",
    "jet-paths": "^1.0.6",
    "jet-validator": "^1.1.1",
    "jsonfile": "^6.1.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.2",
    "pug": "^3.0.2",
    "ts-command-line-args": "^2.5.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.4",
    "@types/jsonfile": "^6.1.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.29",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "jasmine": "^5.1.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2"
  }
}
